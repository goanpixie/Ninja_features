
<h1>Create new User</h1>
<p id="notice"><%= notice %></p>
<%= @users.each do |problem| %>
    <%= problem.name %>
<% end %>

<%= form_for Problem.new do |f| %>
  <div class="field">
    <%= f.label :name %><br>
    <%= f.text_field :name %>
  </div>
  <div class="field">
    <%= f.label :description %><br>
    <%= f.text_field :description %>
  </div>

  <div class="actions">
    <%= f.submit %>
  </div>
<% end %>
<%= link_to 'Back', users_path %>

